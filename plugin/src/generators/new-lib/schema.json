{
  "$schema": "http://json-schema.org/schema",
  "$id": "NewLib",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What is the name for the library?"
    },
    "github": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 1
      },
      "x-prompt": "What is the Github repository for the library? (i.e: eneajaho/ngx-libs)"
    }
  },
  "required": ["name"]
}
